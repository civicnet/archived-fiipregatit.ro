<div
  class="container-fluid"
  id="video-gallery-section"
  style="{{# bg }} background: {{ . }} {{/ bg }}">
  <div class="container">
    <h2>
      Galerie Video
    </h2>
    <div class="row video-gallery-row">
      {{# videos }}
        <div class="video-gallery-box col-lg-4 col-md-6 col-sm-12 col-xs-12">
          <div class="video-gallery-content">
            <h3>
              {{ title }}
            </h3>
            {{# video }}
              <div class="extra-video">
                {{{ video }}}
              </div>
            {{/ video}}
            <a
              href="{{ permalink }}"
              role="button"
              class="outline-button btn btn-outline-secondary rounded-0 campaign-button">
              Vezi Ghid {{ title }}
              <i class="fa fa-chevron-right" aria-hidden="true"></i>
            </a>
          </div>
        </div>
      {{/ videos }}
    </div>
  </div>
</div>
